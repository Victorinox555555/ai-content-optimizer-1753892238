{
  "version": 2,
  "name": "ai-content-optimizer",
  "builds": [
    {
      "src": "wsgi.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/wsgi.py"
    }
  ],
  "env": {
    "FLASK_ENV": "production"
  },
  "functions": {
    "wsgi.py": {
      "runtime": "python3.9"
    }
  }
}